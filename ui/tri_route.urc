//--- solve button
resource Button
{
	name "tri_route_base_button_solve"
	rect 0 80 100 20
	fgcolor 0.00 0.00 0.00 1.00
	hoverbgcolor 1 1 1 1
	bgcolor 0.35 0.67 1.00 1.00
	borderstyle "INDENT_BORDER"
	title "DEVSKIP"
	cvarvisibility developer 1
	ServerThreadToRun globalTricorderRoute_Solve
}

//--- tooltip
resource Label
{
	name "tri_ro_tooltip"
	rect 233 98 174 20
	fgcolor 0.75 0.75 0.9 0.90
	bgcolor 0 0 0 0
	font serpentinemedium-8
}

resource 
Label
{
	name "tri_kp_title"
	rect 213 33 220 30
	fgcolor 1.0 1.0 1.0 1.00
	bgcolor 0.15 0.47 0.60 0.00
