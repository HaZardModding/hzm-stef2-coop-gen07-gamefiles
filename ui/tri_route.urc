menu "TricorderRoute" 640 480 NONE 0.5
align  center
bgcolor 0 0 0 0.9
borderstyle NONE
direction from_top .1
showcommand "ui_showmouse\n"
hidecommand "ui_hidemouse\n"
fadein 0.75
noEscape 1
depth 0.35
fullscreen 1

