textures/def_trans/dt_wall01
{
	qer_editorimage textures/def_trans/dt_wall01.tga
	surfaceparm nomarks
	{
		map $lightmap 
		rgbGen identity
		tcGen lightmap 
	}
	{
		map textures/def_trans/dt_wall01.tga
		blendfunc filter
		rgbGen identity
	}
	{
		map textures/def_trans/dt_wall01a.tga
		blendfunc add
		rgbGen wave sin 0.7 0.1 0 0.4 
	}
}

