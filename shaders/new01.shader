textures/stvcoi_lcars/sl_standard06
{
	qer_editorimage textures/stvcoi_lcars/sl_standard06.tga
	surfaceparm nomarks
	polygonoffset
	{
		map textures/stvcoi_lcars/sl_standard06.tga
		blendfunc add
		rgbGen identity
	}
	{
		map textures/stvcoi_anm/sl_standard06a.tga
		blendfunc add
		rgbGen wave sin 1 0.5 0 0.5 
	}
}

